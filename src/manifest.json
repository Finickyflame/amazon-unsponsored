{
    "name": "Amazon Unsponsored",
    "description": "Remove all sponsored links in amazon",
    "version": "1.1",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://*.amazon.com.au/*",
                "https://*.amazon.com.br/*",
                "https://*.amazon.ca/*",
                "https://*.amazon.cn/*",
                "https://*.amazon.eg/*",
                "https://*.amazon.fr/*",
                "https://*.amazon.de/*",
                "https://*.amazon.in/*",
                "https://*.amazon.it/*",
                "https://*.amazon.co.jp/*",
                "https://*.amazon.com.mx/*",
                "https://*.amazon.nl/*",
                "https://*.amazon.pl/*",
                "https://*.amazon.sa/*",
                "https://*.amazon.sg/*",
                "https://*.amazon.es/*",
                "https://*.amazon.se/*",
                "https://*.amazon.com.tr/*",
                "https://*.amazon.ae/*",
                "https://*.amazon.co.uk/*",
                "https://*.amazon.com/*"
            ],
            "css": [
                "injection.css"
            ]
        }
    ]
}